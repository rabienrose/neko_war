; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Character",
"class": "Building",
"language": "GDScript",
"path": "res://objects/building.gd"
}, {
"base": "Node2D",
"class": "Character",
"language": "GDScript",
"path": "res://objects/character.gd"
} ]
_global_script_class_icons={
"Building": "",
"Character": ""
}

[application]

config/name="NekoWar"
run/main_scene="res://ui/splash.tscn"
config/icon="res://icon.png"

[autoload]

Global="*res://global.gd"

[display]

window/size/width=2364
window/size/height=1182
window/size/test_width=1200
window/size/test_height=600
window/stretch/mode="2d"
window/stretch/aspect="keep_height"

[input_devices]

pointing/emulate_touch_from_mouse=true

[physics]

common/physics_fps=50
common/enable_pause_aware_picking=true

[rendering]

2d/options/ninepatch_mode=0
environment/default_environment="res://default_env.tres"
